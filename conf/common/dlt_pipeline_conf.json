{
    "name": "{{ dlt_pipeline_name }}",
    "clusters": [
        {
            "label": "default",
            "autoscale": {
                "min_workers": 8,
                "max_workers": 32,
                "mode": "ENHANCED"
            },
            "node_type_id": "Standard_L8s_v3",
            "driver_node_type_id": "Standard_F64s_v2"
        }
    ],
    "development": false,
    "continuous": false,
    "channel": "CURRENT",
    "edition": "ADVANCED",
    "photon": false,
    "configuration": {
        "pipeline.requirements_filepath": "{{ requirements_filepath }}",
        "pipeline.host": "{{ host }}",
        "pipeline.env": "{{ env }}",
        "pipeline.unique_repo_branch_id": "datta",
        "pipeline.repos_path": "{{ repos_path }}",
        "pipeline.exchange_rate_type": "001M",
        "pipeline.currency": "USD",
        "pipeline.transactional_table": "VBRP_VBRK",
        "pipeline.date_field": "Billing_Document_Date",
        "pipeline.amount_field": "NetValue_BillingItem",
        "pipeline.currency_field": "Document_Currency",
        "pipeline.mode": "LTOG",
        "pipeline.AltUOM": "TO,LB",
        "pipeline.MaterialNoField": "MATNR",
        "pipeline.QuantityField": "FKIMG",
        "pipeline.UOMField": "MEINS",
        "pipeline.TransactionalTable": "VBRP",
        "pipeline.ConversionMode": "T006",
        "pipeline.load_type":"DELTA",
        "pipeline.full_load_source":"HANA",
        "pipeline.delta_load_source":"AECORSOFT"

    },
    "libraries": [
        {
            "notebook": {
                "path": "{{ repos_path }}/dlt_pipelines/install_requirements"
            }
        },
        {
            "notebook": {
                "path": "{{ repos_path }}/dlt_pipelines/reference_pipeline_main"
            }
        }
    ]
}
